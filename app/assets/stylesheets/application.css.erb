 body{
 	margin: 0;
 }

.floor-container{
	background-color: #EEE;
	overflow: scroll;
	-webkit-overflow-scrolling: touch;

	position: relative;
	left: 0;
	right: 0;
}

.room-object{
	position: absolute;

	-moz-user-select: none;
	-webkit-user-select: none;
	-ms-user-select:none;
	user-select:none;
	-o-user-select:none;
}

.room-object.block{
	background-image: url("<%= asset_path('block_bg.png') %>");

	background-color: #F76868;
}

.room-object.walkable{
	background-image: url("<%= asset_path('walkable_bg.png') %>");

	background-color: #59BDCE;
}

.room-object.elevator{
	background-image: url("<%= asset_path('elevator_bg.png') %>");

	background-color: #E8CF96;

	background-position: center;
    background-repeat: no-repeat;
    background-size: cover;

    /*width: 100px;
    height: 100px;*/
}

.room-object.stair{
	background-color: #bbb;

	background-position: center;
    background-repeat: no-repeat;
    background-size: cover;

    /*width: 100px;
    height: 100px;*/
}

.room-object.stair.up{
	background-image: url("<%= asset_path('stairs_up_bg.png') %>");
}

.room-object.stair.down{
	background-image: url("<%= asset_path('stairs_down_bg.png') %>");
}

.room-object.room{
	/*width:100px;
	height: 100px;*/

	cursor: pointer;

	background-color: #AACE9E;
	background-position: center;
    background-repeat: no-repeat;
}

.room:active{
	background-color: #ffeeaa;
}

.room-object.active{
	background-color: #DEAEA4;
}

.floor-container::-webkit-scrollbar-track
{
	background-color: #F5F5F5;
}

.floor-container::-webkit-scrollbar
{
	width: 5px;
	height: 5px;
	background-color: #F5F5F5;
}

.floor-container::-webkit-scrollbar-thumb
{
	background-color: #555;
}

.opacity-modal{
	opacity: 0.85!important;
}

.room .notes{
	background-color: #fff;
	opacity: 0.8;
	overflow: hidden;

	margin: 2px;
	padding: 2px;
}

/*.modal-dialog, .modal-content {
	height: 100%;
}*/

.screen-block{
	position: fixed;
	left: 0;
	top: 0;

	background-color: #ddd;
	opacity: 0;

	width:100%;
	height: 100%;

	z-index: 99999;
}

.screen-block.real-block{
	opacity: 0.9;
	color: #fff;

	display: table;
}

.screen-block.real-block .cell{
	display: table-cell;
	vertical-align: middle;
}

.screen-block.real-block .info{
	width: 250px;
	margin: auto;
	padding: 10px;

	text-align: center;
}

.screen-block.real-block .info.small{
	color: #aaa;
}

.screen-block.real-block img{
	width: auto;
	margin: auto;
}

.screen-block.real-block a{
	text-decoration: underline;
}
.screen-block.real-block a:hover{
	color: #ddd;
}

a.underline_link{
	text-decoration: underline;
}

.eac-item b{
	color: #26a69a;
}

.eac-item {
	font-size: 32px;
}

.footer-copyright .right{
	margin-left: 30px;
}

.left-text{
	text-align: left;
}

.btn-blue{
	background-color: #266BA6;
}

.btn-blue:focus{
	background-color: #2A5CB7;
	color:#fff;
}

.btn-blue:hover{
	background-color: #2B72BB;
	color:#ddd;
}

.floor-controls{
	height: 70px;
	width: 100%;

	overflow-x: auto;
	overflow-y: hidden;
	
	background-color: #aaa;
}

.control-div{
	height: 70px;
	background-color: #bbb;
	color: #333;
	margin-right: 5px;

	text-align: center;
	display: table;

	float: right;
}

.control-div[hidden], .waves-effect[hidden]{
	display: none;
}

.control-div.active{
	background-color: #B6DEEA;
	color: #3A7888;
}

.control-div.fixed-width{
	width: 70px;
}

.control-div .control-content{
	display: table-cell;
	vertical-align: middle;
}

textarea.normal{
	height: inherit;
}

.room-mark{
	position: absolute;
	width: 100%;
	bottom: -3px;
}

.room-mark > span {
	float:right;
	margin-right: 5px;
}

.room-mark i{
	font-size: 18px;
}

.text-success{
	color: #3c763d;
}

.floor-name{
	font-size: 16px;
	text-decoration: underline;
	margin-left: 10px;
	margin-right: 5px;
}

.floor-name.editable{
	font-size: 22px;
}

.floor-name-edit i{
	font-size: 16px;
}

.floor-label{
	margin-left: 20px;
}

.header-action{
	cursor: pointer;
}

.delete-btn{
	cursor: pointer;
}

.sidebar-item-control{
	float: right;
}

.sidebar-item-control .action-icon{
	margin-right: 0!important;
}

.sidebar-item-control .action-icon.pending{
	color: #FC0;
}

.sidebar-item-control .action-icon.started{
	color: #00FFD4;
}

.status-display-cavnas{
	position: absolute;
	top: 0px;
	left: 0px;
}

.login-form-content{
	margin-top: 64px;
}

.form-content{
	margin-top: 32px;
	margin-bottom: 32px;
}

.field_with_errors{
	color: #f00;
}

.field_with_errors label{
	color: #f00;
}

.normal-img img{
	width: auto;
	display: inline-block;
}

.rate-btn{
	height: 80px;
    line-height: 26px;
    margin-right: 20px;
}

.rate-btn:last-child{
	margin-right: 0px!important;
}

.rate-btn i{
	font-size: 50px;
}

.rate-btn.like-btn{
	background-color: #FF6767;
    color: #FFE7E7;
}

.rate-btn.dislike-btn{
	background-color: #0F308E;
    color: #A0C0FF;
}

.rates-label{
    padding: 0 10px;
    border-radius: 5px;
}

.rates-label.like-rates{
	color: #FF6767;
    background-color: #FFE7E7;
}

.rates-label.dislike-rates{
	background-color: #A0C0FF;
    color: #0F308E;
}

.modal-header.compact{
	padding: 5px 15px;
}

.rate-title{
	text-decoration: underline;
	margin-bottom: 10px;
}

.report-btn{
	font-size: 12px;
    padding: 0 10px;
}

.complete-link{
	background-color: #498298!important;
}

.rate-block{
	display: inline-block;
	margin-right: 0px;
}

.rate-block:first-child{
	margin-right: 20px!important;
}

.stats-info{
	margin-top: 15px;
	margin-bottom: 15px;
}

.opacity-modal {
	-moz-user-select: none;
	-webkit-user-select: none;
	-ms-user-select:none;
	user-select:none;
	-o-user-select:none;
}

.search-label{
	font-size: 18px;
	color:#26a69a;
}

.able-color{
	color: #87d07c;
}

.disable-color{
	color: #ddd;
}

.btn-grey{
	background-color: #bdbdbd;
	color:#000;
}

.btn-grey:focus{
	background-color: #989898;
	color:#000;
}

.btn-grey:hover{
	background-color: #989898;
	color:#333;
}

.btn-yellow{
	background-color: #ffe52a;
	color:#5a2b00;

	box-shadow: 0 2px 5px 0 rgba(158, 154, 29, 0.16),0 2px 10px 0 rgba(138, 134, 24, 0.12);
}

.btn-yellow:focus{
	background-color: #ffed6d;
	color:#5a2b00;
}

.btn-yellow:hover{
	background-color: #ffed6d;
	color:#5a2b00;

	box-shadow: 0 5px 11px 0 rgba(158, 104, 4, 0.18),0 4px 15px 0 rgba(56, 115, 11, 0.15);
}

.btn-red{
	background-color: #ffe52a;
	color:#5a2b00;

	box-shadow: 0 2px 5px 0 rgba(158, 154, 29, 0.16),0 2px 10px 0 rgba(138, 134, 24, 0.12);
}

.btn-red:focus{
	background-color: #ffed6d;
	color:#5a2b00;
}

.btn-red:hover{
	background-color: #ffed6d;
	color:#5a2b00;

	box-shadow: 0 5px 11px 0 rgba(158, 104, 4, 0.18),0 4px 15px 0 rgba(56, 115, 11, 0.15);
}

table.plan-table td, table.plan-table th{
	text-align: center;
}

.plan-table tr > td:first-child{
	min-width: 200px;
}

.plan-container{
	width: 100%;
	overflow-x: scroll;
}

hr.primary-hr{
	border-top: 1px solid #1c212a;
}

.description-section{
	background-color: #eee;
	padding: 10px;

	margin: 30px 10px;
}

.profile-row {
	margin: 10px 0;
	padding: 5px 0;

	background-color: #eee;
}

.text-highlight{
	color: #d4aa00;
}

button.not-verified{
	background-color: #cc7676;
    border: none;
    padding: 5px 20px;
    color: #540808;
}

.verified{
    color: #4d8e6b;
}

.profile-row.qualify{
	background-color: #76cc94;
    color: #06542a;
}

.profile-row.non-qualify{
	background-color: #fdfad2;
    color: #9e8a1d;
}

.toast.toast-info{
	background-color: #ade5ff;
	color: #333;
}

.toast.toast-warning{
	color: #333;
	background-color: #fffad6;
}

.toast.toast-success{
	background-color: #76cc94;
	color: #333;
}

.toast.toast-neutral{
	color: #333;
	background-color: #ddd;
}

.controls button.danger-btn{
	background-color: #ffe6e6;
	border: 1px solid #ab0909;

	color: #ab0909;
}

.controls button.danger-btn:hover, .controls button.danger-btn:active{
	background-color: #fff;
}

.controls button.action-btn{
	background-color: #e6ffef;
    border: 1px solid #09ab3b;

	color: #09ab3b;
}

.controls button.action-btn:hover, .controls button.action-btn:active{
	background-color: #fff;
}

.last-update{
	position: fixed;
	left: 5px;
	top: 62px;

	z-index: 1040;

	background-color: #c5d5e8;
	opacity: 0.8;

	padding: 3px 7px;
}